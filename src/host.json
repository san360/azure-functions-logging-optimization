{
  "version": "2.0",
  "logging": {
    "fileLoggingMode": "debugOnly",
    "logLevel": {
      "default": "Information",
      "Host.Results": "Information",
      "Host.Aggregator": "Trace",
      "Function": "Information",
      "Function.queue_message": "Information",
      "Function.queue_message.User": "Debug",
      "Azure.Core": "Warning",
      "Azure.Storage": "Warning",
      "Azure.Identity": "Warning",
      "Host.Function.Console": "Warning"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "maxTelemetryItemsPerSecond": 5,
        "excludedTypes": "Request",
        "includedTypes": ""
      },
      "enableDependencyTracking": true,
      "dependencyTrackingOptions": {
        "enableSqlCommandTextInstrumentation": false
      },
      "enableLiveMetrics": true,
      "enablePerformanceCountersCollection": true
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.0.0, 5.0.0)"
  },
  "extensions": {
    "queues": {
      "maxPollingInterval": "00:00:30",
      "maxDequeueCount": 1,
      "visibilityTimeout": "00:01:00",
      "batchSize": 16,
      "newBatchThreshold": 8,
      "messageEncoding": "none"
    }
  }
}
